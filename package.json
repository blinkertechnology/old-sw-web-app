{
  "name": "babi2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "env-cmd -f ./.env.js vue-cli-service serve",
    "serve:staging": "env-cmd -f ./.env.staging.js vue-cli-service serve",
    "serve:prod": "env-cmd -f ./.env.prod.js vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "publish": "env-cmd -f ./.env.js yarn build && npm run release",
    "publish:staging": "env-cmd -f ./.env.staging.js yarn build  && npm run release",
    "publish:prod": "env-cmd -f ./.env.prod.js yarn build  && npm run release",
    "release": "gulp -f ./tools/gulpfile.js --target 2.5 && gulp -f ./tools/gulpfile.js --target 3"
  },
  "dependencies": {
    "@nuintun/qrcode": "^3.3.0",
    "@venly/connect": "^1.33.0",
    "@vuelidate/core": "^2.0.0-alpha.41",
    "@vuelidate/validators": "^2.0.0-alpha.29",
    "axios": "^0.27.2",
    "barcode-detector": "^1.0.0",
    "core-js": "^3.6.5",
    "crypto-js": "^4.1.1",
    "crypto-md5": "^1.0.0",
    "cryptocompare": "^1.0.0",
    "etherscan-api": "^10.2.0",
    "jquery": "^3.6.0",
    "js-base64": "^3.7.2",
    "js-md5": "^0.7.3",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "qrcode-decoder": "^0.2.2",
    "simple-camera-service": "^1.0.2",
    "vue": "^2.6.11",
    "vue-axios": "^3.4.1",
    "vue-axios-cors": "^1.0.1",
    "vue-cookies": "^1.8.2",
    "vue-i18n": "^8.28.2",
    "vue-kaiui": "^0.1.1",
    "vue-qrcode-component": "^2.1.1",
    "vue-qrcode-reader": "^3.1.0",
    "vue-router": "^3.5.3",
    "vue-session": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@types/js-md5": "^0.4.3",
    "@types/node": "^17.0.38",
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-eslint": "~5.0.4",
    "@vue/cli-service": "^5.0.8",
    "del": "^7.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-template": "^5.0.0",
    "gulp-zip": "^5.1.0",
    "http-server": "^0.12.3",
    "vue-template-compiler": "^2.6.11",
    "yargs": "^17.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-unused-vars": "off",
      "vue/multi-word-component-names": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "typings": "dist/index"
}
